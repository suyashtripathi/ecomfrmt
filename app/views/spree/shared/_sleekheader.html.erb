              <button type="button" id="clear-ta-content" class="btn btn-gray-200 w-6rem d-flex align-items-center justify-content-center">
                <figure class="figure">
                  <svg class="svg-icon service-icon">
                    <use xlink:href="#clear-box"> </use>
                  </svg>
                  <figcaption class="figure-caption text-center">Clear</figcaption>
                </figure>
              </button>
            </div>
            <button type="button" aria-label="Close" class="close modal-close">
              <svg class="svg-icon w-100 h-100 svg-icon-light align-middle">
                <use xlink:href="#close-1"> </use>
              </svg>
            </button>
            <!--<div class="col-7">
